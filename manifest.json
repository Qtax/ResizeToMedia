{
	"name": "Resize To Media",
	"version": "0.5",
	"description": "Resize browser window to the size of the largest media (flash, video, image, etc) element on the current page.",
	
	"browser_action": {
		"default_icon": "img/icon19.png",
		"default_title": "Resize window to media content"
	},
	
	"update_url": "http://www.qtax.se/resizetomedia/updates.xml",
	
	"background_page": "bg.html",
	"options_page": "options.html",
	
	"icons": {
		"128": "img/icon128.png",
		"48":  "img/icon48.png"
	},
	
	"permissions": [
		"tabs",
		"contextMenus",
		"<all_urls>"
	]
}